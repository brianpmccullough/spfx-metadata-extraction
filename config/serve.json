          "location": "ClientSideExtension.ListViewCommandSet.CommandBar",
          "properties": {
            "allowedFileTypes": [".pdf", ".doc", ".docx"],
            "baseApiUrl": "http://localhost:3000"
          }
        }
      }
    }
  }
}
